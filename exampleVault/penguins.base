formulas:
  group: species
views:
  - type: chart-scatter
    name: Chart
    filters:
      and:
        - file.folder == "penguins"
    groupBy:
      property: species
      direction: ASC
    order:
      - culmen_depth_mm
      - flipper_length_mm
    x: note.culmen_length_mm
    multi-chart-mode: Separate by property
    sync-y-axes: false
