formulas:
  year: date(Date).year.toString()
properties:
  formula.year:
    displayName: Year
views:
  - type: chart-line
    name: View
    filters:
      and:
        - file.folder == "aapl"
    groupBy:
      property: formula.year
      direction: ASC
    order:
      - Open
      - Close
    x: note.Date
    multi-chart-mode: Separate by group
    sync-y-axes: true
    min-y-override: "0"
